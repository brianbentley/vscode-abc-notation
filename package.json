{
	"name": "abc-notation",
	"displayName": "ABC Notation",
	"description": "Syntax highlighting and web view for displaying ABC musical notation.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.1"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:abcNotation.start",
		"onCommand:abcNotation.doRefactor",
		"onWebviewPanel:abcNotation"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "abcNotation.start",
				"title": "Start ABC Notation session",
				"category": "ABC Notation"
			},
			{
				"command": "abcNotation.doRefactor",
				"title": "Do some refactoring",
				"category": "ABC Notation"
			}
		],
		"languages": [
			{
				"id": "abc",
				"aliases": [
					"ABC Notation",
					"abc"
				],
				"extensions": [
					".abc",
					".abh",
					".abp"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "abc",
				"scopeName": "source.abc",
				"path": "./syntaxes/abc-notation.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {}
}
